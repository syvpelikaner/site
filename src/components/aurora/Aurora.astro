<canvas class="w-full h-full" id="aurora-canvas"></canvas>
<script>
  import { runAurora } from "./aurora.ts";

  const canvas = document.querySelector("#aurora-canvas") as HTMLCanvasElement;

  if (canvas === null || !(canvas instanceof HTMLCanvasElement)) {
    throw new Error("Canvas element not found");
  }

  runAurora(canvas);
</script>
